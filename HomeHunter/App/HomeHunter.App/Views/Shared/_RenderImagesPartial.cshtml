@model HomeHunter.Models.ViewModels.Offer.OfferDetailsViewModel;


@foreach (var image in Model.Images)
{
    <img id="myImg" src="~/images/@image" class="myImages img-thumbnail img-h-block">
    <div id="myModal" class="modal">
        <!-- The Close Button -->
        <span class="close">&times;</span>
        <!-- Modal Content (The Image) -->
        <img class="modal-content" id="img01">
        <!-- Modal Caption (Image Text) -->
        <div id="caption"></div>
    </div>
}